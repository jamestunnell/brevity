module Brevity

grammar Duration
  include Nonzero
  
  rule duration
    num_and_den / num_only / den_only
  end
  
  rule num_and_den
    nonzero_number "/" nonzero_number <NumDenNode>
  end
  
  rule num_only
    numerator:nonzero_number "/"? <NumOnlyNode>
  end
  
  rule den_only
    "/" denominator:nonzero_number <DenOnlyNode>
  end
end

end